array = []
def howSum(targetSum, numbers):
    if (targetSum == 0):
        return []
    if (targetSum < 0):
        return None
        
    for num in numbers:
        remainder = targetSum - num
        remainderResult = howSum(remainder, numbers)
        
        if remainderResult != None:
            array.append(num)
            return array
            
    return None
print(howSum(7, [2,3,5]))
